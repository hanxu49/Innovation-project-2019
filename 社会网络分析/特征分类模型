    "Y2=data_str.iloc[:,2]\n",
    "X_text=data_str.iloc[:,0]\n",
    "X_sim=data_str.iloc[:,4]\n",
    "X_len=data_str.iloc[:,5]\n",
    "X_emo1=data_str.iloc[:,6]\n",
    "X_emo2=data_str.iloc[:,7]\n",
