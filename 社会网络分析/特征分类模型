   ],
   "source": [
    "#文本处理\n",
    "X_text.tolist()\n",
    "from sklearn.feature_extraction.text import TfidfVectorizer\n",
    "#max_df = 0.9 # 在超过这一比例的文档中出现的关键词（过于平凡），去除掉。\n",
    "#min_df = 3 # 在低于这一数量的文档中出现的关键词（过于独特），去除掉\n",
    "tfidf = TfidfVectorizer(max_df=0.8,min_df=10)\n",
    "X_text_vec=pd.DataFrame(tfidf.fit_transform(X_text.values.astype('U')).toarray(),columns=tfidf.get_feature_names())\n",
    "print(X_text_vec.shape)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 数据处理\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "sc = StandardScaler()\n",
    "#length标准化处理\n",
    "X_len_s=np.array(X_len).reshape(-1,1)\n",
    "X_len_s= sc.fit_transform(X_len_s)\n",
    "X_len_s=pd.DataFrame(X_len_s)\n",
    "X_len_s.columns = ['length']\n",
    "#freq_re\n",
    "X_freq_re_s=np.array(X_freq_re).reshape(-1,1)\n",
    "X_freq_re_s= sc.fit_transform(X_freq_re_s)\n",
    "X_freq_re_s=pd.DataFrame(X_freq_re_s)\n",
    "X_freq_re_s.columns =[ 'X_freq_re']\n",
    "#freq_th\n",
    "X_freq_th_s=np.array(X_freq_th).reshape(-1,1)\n",
    "X_freq_th_s= sc.fit_transform(X_freq_th_s)\n",
    "X_freq_th_s=pd.DataFrame(X_freq_th_s)\n",
    "X_freq_th_s.columns = ['X_freq_th']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "#topic+linguistic\n",
    "X1=X_topic.join(X_len_s)\n",
    "X2=X1.join(X_http)\n",
    "X3=X2.join(X_jpg)\n",
    "X4=X3.join(X_sim)\n",
    "#topic+linguistic+emo\n",
    "X5=X4.join(X_emo1)\n",
    "#X6=X5.join(X_emo2)\n",
    "X7=X5.join(X_emo3)\n",
    "#topic+linguistic+emo+sim\n",
    "#X8=X7.join(X_sim)\n",
    "#topic+linguistic+emo+sim+behavior\n",
    "X9=X7.join(X_freq_re_s)\n",
    "X10=X9.join(X_freq_th_s)\n",
    "X01=X10.join(X_degree)\n",
    "X02=X01.join(X_betweeness)\n",
    "X03=X02.join(X_closeness)"
   ]
