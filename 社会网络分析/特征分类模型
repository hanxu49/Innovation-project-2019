    "X_train, X_test, Y_train, Y_test=train_test_split(X05, Y, test_size=0.2, random_state=0)\n",
    "#clf = OneVsRestClassifier(LogisticRegression(solver='liblinear'))\n",
    "clf = OneVsRestClassifier(SVC(C=1,kernel='rbf',gamma='scale'))\n",
    "#clf = OneVsRestClassifier(RandomForestClassifier(n_estimators=10, max_depth=None,min_samples_split=2, random_state=0))\n",
    "#clf = OneVsRestClassifier(KNeighborsClassifier(algorithm='auto', leaf_size=30, metric='minkowski',metric_params=None, n_jobs=1, n_neighbors=5, p=2,weights='uniform'))\n",
    "#clf = OneVsRestClassifier(XGBClassifier())\n",
    "X_resampled,y_resampled = ros.fit_sample(X_train, Y_train)\n",
    "#y_pred = clf.fit(X_resampled, y_resampled).decision_function(X_test)\n",
    "clf.fit(X_resampled, y_resampled)\n",
    "X_test=X_test.values\n",
    "#Y_test=Y_test.values \n",
    "y_pred = clf.predict(X_test)\n",
    "#结果转换\n",
    "for i in range(len(y_pred)):\n",
    "        max_value=max(y_pred[i])\n",
    "        for j in range(len(y_pred[i])):\n",
    "            if max_value==y_pred[i][j]:\n",
    "                y_pred[i][j]=1\n",
    "            else:\n",
    "                y_pred[i][j]=0\n",
    "print(y_pred.shape)\n",
    "\n",
    "print(classification_report(Y_test, y_pred,digits=3,sample_weight=None))#[0,1,2,3,4]\n",
    "\n",
    "\n",
    "#f1 = f1_score( Y_test, y_score, average='macro' )\n",
    "#print(f1)"
   ]
