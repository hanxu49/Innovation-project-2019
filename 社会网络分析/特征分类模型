   "source": [
    "'''\n",
    "#数据处理\n",
    "df=pd.read_csv(\"data_5000.csv\",encoding='gbk')\n",
    "X=df.iloc[:,2]\n",
    "length=dict()\n",
    "for i in range(len(X)):\n",
    "    length[i]=len(X.iloc[i])      \n",
    "# 将字典的value转换成列表\n",
    "lst = list(length.values())\n",
    "from pandas.core.frame import DataFrame\n",
    "lst=DataFrame(lst)\n",
    "df=df.join(lst) \n",
    "df.to_csv('C:/Users/xiaoxin/tieba/data2.csv',sep=',',encoding='gbk',index=False,header=True)  #index=False,header=False表示不保存行索引和列标题\n",
    "'''"
   ]
