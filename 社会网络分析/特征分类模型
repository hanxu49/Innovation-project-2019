   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "mean acc: 0.7957544637084444\n",
      "mean f1：\t 0.8051742077009723\n",
      "mean precision：\t 0.8185714764724485\n",
      "mean recall ：\t 0.7957544637084444\n",
      "mean auc： 0.652801013930689\n"
     ]
    }
   ],
   "source": [
    "#random sampling使用RandomOverSampler从少数类的样本中进行随机采样来增加新的样本使各个分类均衡\n",
    "from imblearn.over_sampling import RandomOverSampler\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn import tree\n",
    "from sklearn.naive_bayes import GaussianNB,MultinomialNB,BernoulliNB\n",
    "from sklearn.ensemble import AdaBoostClassifier\n",
    "import xgboost\n",
    "from xgboost import XGBClassifier\n",
    "from sklearn.neighbors import KNeighborsClassifier\n",
    "ros = RandomOverSampler(random_state=0)\n",
    "cvscores_acc= []\n",
    "cvscores_f1= []\n",
    "cvscores_p= []\n",
    "cvscores_r= []\n",
    "cvscores_auc=[]\n",
    "skf = StratifiedKFold(n_splits=10,random_state=8,shuffle=False)\n",
    "for train_index,test_index in skf.split(X_text_vec, Y2):\n",
    "    X_train, X_test =X_text_vec.iloc[train_index],X_text_vec.iloc[test_index]\n",
    "    Y_train, Y_test = Y2[train_index], Y2[test_index]\n",
    "    X_resampled, y_resampled = ros.fit_sample(X_train, Y_train)\n",
    "    X_test=X_test.values\n",
    "    Y_test=Y_test.values \n",
    "    #clf=LogisticRegression(solver='liblinear')#逻辑回归\n",
    "    #clf=SVC(C=1,kernel='rbf',gamma='scale')#,class_weight='balanced'\n",
    "    clf= RandomForestClassifier(n_estimators=10, max_depth=None,min_samples_split=2, random_state=0)#随机森林\n",
    "    #clf = XGBClassifier()\n",
    "    #clf =KNeighborsClassifier(algorithm='auto', leaf_size=30, metric='minkowski',metric_params=None, n_jobs=1, n_neighbors=5, p=2,weights='uniform')\n",
    "    clf.fit(X_resampled, y_resampled)\n",
    "    #clf.fit(X_train, Y_train)\n",
    "    y_pred =clf.predict(X_test)\n",
    "    #y_pred= [round(value) for value in y_pred]\n",
    "    acc=accuracy_score(Y_test, y_pred)\n",
    "    cvscores_acc.append(acc)\n",
    "    f1=f1_score(Y_test, y_pred,average='weighted')#,average='weighted'\n",
    "    cvscores_f1.append(f1)\n",
    "    p=precision_score(Y_test, y_pred,average='weighted')#,average='weighted'\n",
    "    cvscores_p.append(p)\n",
    "    r=recall_score(Y_test, y_pred,average='weighted')#,average='weighted'\n",
    "    cvscores_r.append(r)\n",
    "    auc=roc_auc_score(Y_test, y_pred)#,average='weighted'\n",
    "    cvscores_auc.append(auc)\n",
    "print(\"mean acc:\",np.mean(cvscores_acc))\n",
    "print ('mean f1：\\t',np.mean(cvscores_f1))\n",
    "print ('mean precision：\\t',np.mean(cvscores_p))\n",
    "print ('mean recall ：\\t',np.mean(cvscores_r))\n",
    "print ('mean auc：',np.mean(cvscores_auc))"
   ]
  },
